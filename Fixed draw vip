    <script>
        function revealAllText() {
            const password = prompt("Enter the password:");
            if (password === "0188") {
                const blurredElements = document.querySelectorAll('.blur-text');
                blurredElements.forEach(element => {
                    element.style.filter = "none";
                    element.style.cursor = "default";
                });
                document.querySelectorAll('.blur-text').forEach(el => el.removeEventListener('click', revealAllText));
            } else {
                alert("Incorrect password!");
            }
        }

        window.onload = function() {
            document.querySelectorAll('.blur-text').forEach(el => el.addEventListener('click', revealAllText));
        }

        document.addEventListener('contextmenu', event => event.preventDefault()); // Disable right-click context menu
    </script>
</head>
